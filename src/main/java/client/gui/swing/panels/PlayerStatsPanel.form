<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="client.gui.swing.panels.PlayerStatsPanel">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="1101" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="41b6c" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="e0aab" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text value="Select Player"/>
            </properties>
          </component>
          <hspacer id="9522f">
            <constraints/>
          </hspacer>
          <component id="413e" class="javax.swing.JComboBox" binding="playerList">
            <constraints/>
